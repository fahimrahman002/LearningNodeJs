<script>
    toastr.options.timeOut = `{{msgtime}}` || 1000;
    const msgtype = `{{msgtype}}`
    switch (msgtype) {
        case "success":
            toastr.success(`{{msg}}`);
            break;
        case "warning":
            toastr.warning(`{{msg}}`);
            break;
        case "error":
            toastr.error(`{{msg}}`);
            break;
    }
</script>